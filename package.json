{
  "private": true,
  "license": "ISC",
  "version": "0.0.26",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.18.4",
    "mobx": "^6.3.12",
    "mobx-react-lite": "^3.2.3",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.20",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "cz-conventional-changelog": "^3.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
